<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping SYSTEM
        "hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ServerSide.Databases.DataTables.Cena" table="Cena">
        <id name="idCena" column="idCena">
